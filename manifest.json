{
    "name": "Dynamic Bookmark",
    "version": "0.1",
    "description": "My first extension lol",
    "permissions": [
        "storage",
        "bookmarks",
        "tabs"
    ],
    "browser_action": {
        "default_icon": {
            
        },
        "default_title": "Dynamic Bookmarks",
        "default_popup": "popup/index.html"
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ],
        "persistent": false
    },
    "manifest_version": 2
}