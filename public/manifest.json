{
  "name": "Dynamic Bookmark",
  "version": "0.0.1",
  "description": "My first extension lol",
  "permissions": [
      "bookmarks",
      "storage"
  ],
  "browser_action": {
      "default_icon": {
          
      },
      "default_title": "Dynamic Bookmarks",
      "default_popup": "index.html"
  },
  "background": {
      "scripts": [
          "background.js"
      ],
      "persistent": false
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-QM+HKPczd43Ke++ZMRCqNOUxYEg1Q8bHX/tSRAWAlwM='; object-src 'self'",
  "manifest_version": 2
}
